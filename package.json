{
  "name": "angular-multimodule-cli",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "Ryanair DAC Developers",
  "bin": {
    "ng-multi": "./bin/ng-multi"
  },
  "scripts": {
    "cz": "git-cz",
